					LoveNPC: "{$Mods.HobbitonMod.TownNPCMood.Hobbit.LoveNPC}"
					LikeNPC: "{$Mods.HobbitonMod.TownNPCMood.Hobbit.LikeNPC}"
					LikeNPC_Princess: "{$Mods.HobbitonMod.TownNPCMood.Hobbit.LikeNPC_Princess}"
